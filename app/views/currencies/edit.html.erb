<% @title='Edit Currency' %>

<h3>Edit Currency:</h3>
<% form_for(@currency,:url=>{ :action => "update" },:method => "put") do |f| %>
	<%= render(:partial => 'f', :object => f) %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', currency_path(@currency) %> |
<%= link_to 'Back', currencies_path %>
