<h3>Create A New Account</H3>

<%= error_messages_for :user %>

<% form_for(:user, :url => users_path) do |f| %>

  <p><%= f.label :user_name, 'Account Name:'%> <%= f.text_field :user_name %></p>

  <%= render(:partial => 'form', :object => f) %>

  <p>
    <%= submit_tag "Create account" %>
  </p>
<% end %>
