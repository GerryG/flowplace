<h1>New proposal</h1>

<% form_for(@proposal) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :as %><br />
    <%= f.text_field :as %>
  </p>
  <p>
    <%= f.label :weal_id %><br />
    <%= f.text_field :weal_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', proposals_path %>
